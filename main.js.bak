				return await message.reply("Error playing url, is it a valid audio source?")
			}
		}
		voices.set(message.guild.id, dispatcher);
		dispatcher.on('finish', () => {
			voices.delete(message.guild.id);
			dispatcher.destroy();
			message.member.voice.channel.leave();
		});
		
